{
    "train_micro_batch_size_per_gpu": 128,

    "steps_per_print": 25,

    "bf16": {
      "enabled": true
    },

    "optimizer": {
        "type": "Adam",
        "params": {
          "lr": 2e-5,
          "betas": [
            0.9,
            0.999
          ],
          "eps": 1e-8,
          "weight_decay": 3e-7
        }
    },

    "scheduler": {
      "type": "WarmupLR",
      "params": {
          "warmup_min_lr": 0,
          "warmup_max_lr": 2e-5,
          "warmup_num_steps": 50000
        }
    },
  


    "wandb": {
      "enabled": true,
      "project": "Cheatsheet"
    }
}